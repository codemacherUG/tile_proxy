{
  "compilerOptions": {
    "outDir": "./Resoures/Public/JavaScript",
    "noImplicitAny": true,
    "moduleDetection": "force",
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./.build/node_modules",
    "paths": {
      "*": [
        "../@types/*",
        "../node_modules/@types/*",
        "../node_modules/*"
      ]
    },
  }
}